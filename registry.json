{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "Uploadthing UI",
  "homepage": "https://uploadthing-ui.vercel.app",
  "items": [
    {
      "name": "button-proton",
      "type": "registry:component",
      "title": "Proton Drive Button",
      "description": "A button taken inspiration from the proton drive UI",
      "registryDependencies": ["button", "table", "badge", "alert-dialog"],
      "dependencies": ["zustand@latest", "uploadthing@latest", "uploadthing/react@latest", "paralleldrive/cuid2@latest", "lucide-react"],
      "files": [
        {
          "path": "registry/new-york/button-proton/button-proton.tsx",
          "type": "registry:component"
        },
        {
          "path": "store/button-proton-store.ts",
          "type": "registry:file",
          "target": "store/button-proton-store.ts"
        }
      ]
    },
    {
      "name": "ut-button-uploadthing",
      "type": "registry:component",
      "title": "Uploadthing Button",
      "description": "A button taken inspiration from the uploadthing UI",
      "registryDependencies": ["button", "sonner"],
      "dependencies": ["paralleldrive/cuid2@latest", "zustand@latest", "uploadthing@latest", "uploadthing/react@latest", "lucide-react"],
      "files": [
        {
          "path": "registry/new-york/ut-button-uploadthing/ut-button-uploadthing.tsx",
          "type": "registry:component"
        }
      ]
    }
  ]
}